{
    "manifest_version": "2",
    "api_version": "1",
    "name": "Awesome Lists",
    "description": "Search awesome lists",
    "developer_name": "Bruno Paz",
    "icon": "images/icon.png",
    "options": {
        "query_debounce": 0.1
    },
    "preferences": [
        {
            "id": "kw",
            "type": "keyword",
            "name": "Awesome Lists",
            "default_value": "aw"
        },
        {
            "id": "github_access_token",
            "type": "input",
            "name": "Your GitHub access_token [We use token to fetch the lists].",
            "default_value": ""
        }
    ]
}